{
  "name": "resha",
  "version": "0.0.1",
  "description": "Generic project boilerplate for TypeScript",
  "author": "Amir Arad (greenshade@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amir-arad/resha/issues"
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:amir-arad/resha.git"
  },
  "files": [
    "dist/src"
  ],
  "homepage": "https://github.com/amir-arad/resha#readme",
  "keywords": [
    "typescript",
    "tdd"
  ],
  "main": "dist/src/index.js",
  "scripts": {
    "lint": "tslint --project .",
    "watch": "watchify src/client.ts -p [ tsify ] -o=static/client.js",
    "build": "browserify src/client.ts -p [ tsify ] -o=static/client.js",
    "test": "npm run lint && npm run test-only",
    "test-only": "run-s test:node test:browser | tap-merge | tap-set-exit",
    "pretest:e2e": "npm run build && docker-compose -f ./test/e2e/env/docker-compose.yml up --build -d && wait-port localhost:31337",
    "test:e2e": "ts-node node_modules/tape/bin/tape test/e2e/cases/*.spec.ts | tap-spec",
    "test:node": "ts-node node_modules/tape/bin/tape \"test/*/*.spec.ts?(x)\"",
    "test:browser": "node ./scripts/browser-test.js | tape-run",
    "start": "ts-node ./scripts/serve.ts",
    "start:dev": "ts-node --typeCheck ./scripts/dev-serve.ts",
    "start:test": "budo test/browser-env.ts test/*.spec.ts -s=test.bundle --live -p 8080 -- --debug -p [ browserify-hmr ] -p [ tsify ]"
  },
  "devDependencies": {
    "@types/express": "4.11.1",
    "@types/sinon": "4.3.3",
    "@types/tape": "4.2.32",
    "@types/three": "0.92.23",
    "browserify": "16.2.0",
    "browserify-hmr": "0.3.6",
    "browserify-middleware": "8.1.0",
    "budo": "11.2.0",
    "glob": "7.1.3",
    "looks-same": "^3.3.0",
    "npm-run-all": "4.1.3",
    "puppeteer": "^1.4.0",
    "rimraf": "2.6.2",
    "sinon": "5.0.1",
    "tap-colorize": "1.2.0",
    "tap-merge": "0.3.1",
    "tap-set-exit": "1.1.1",
    "tap-spec": "^4.1.1",
    "tape": "4.9.0",
    "tape-run": "4.0.0",
    "tape-worm": "0.0.3",
    "ts-node": "^6.0.0",
    "tsify": "4.0.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "2.8.3",
    "wait-port": "^0.2.2",
    "watchify": "^3.11.0"
  },
  "dependencies": {
    "colyseus": "^0.9.5",
    "colyseus.js": "^0.9.3",
    "express": "4.16.3",
    "pkg-dir": "2.0.0",
    "three": "^0.92.0",
    "tslib": "~1.9.0"
  }
}
